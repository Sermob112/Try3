@model Try3.Models.cars

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Добавить автомобиль</h2>

@using (Html.BeginForm())
{
    @Html.LabelFor(model => model.mark, "Укажите марку вашего автомобиль")
    <br />
    @Html.EditorFor(model => model.mark)
    <br />
    @Html.LabelFor(model => model.carNum, "Укажите номер вашего автомобиль")
    <br />
    @Html.EditorFor(model => model.carNum)
    <br />

    <br />
    <input type="submit" value="Добавить" />
}