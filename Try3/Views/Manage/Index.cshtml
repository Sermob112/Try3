@*@model  Try3.Models.UserDitailView*@
@using Try3.Models
@model dynamic

<!DOCTYPE html>
<html>
<head>
    <title>Парковка</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="../CSS/secondcss.css" />
    <style>
        html, body, h1, h2, h3, h4, h5 {
            font-family: "Raleway", sans-serif
        }
    </style>
</head>
<body class="w3-light-grey">
    @*   @foreach (var item in Model.Orders)
        {
            @Html.DisplayNameFor(modelItem => item.placeId)
            @Html.DisplayFor(modelItem => item.placeId)
        }*@
    @foreach (Try3.Models.orders customer in Model.Orders)
    {
        <tr>
            <td>@customer.carId</td>
            <td>@customer.userId</td>
            <td>@customer.placeId</td>
            <td>@customer.quantity</td>
        </tr>
    }

    <nav class="w3-sidebar w3-collapse w3-white w3-animate-left" style="z-index:3;width:300px; top:110px;" id="mySidebar">
        <br>
        <div class="w3-container w3-row">
            <div class="w3-col s4">
                <img src="../source/Account.png" class="w3-circle w3-margin-right" style="width:46px">
            </div>
            <div class="w3-col s8 w3-bar">
                <span>
                    <strong>  Привет @*@Model.Name*@</strong>
                </span><br>
            </div>
        </div>
        <hr>
        <div class="w3-container">
            <h5>Быстрый Доступ</h5>
        </div>
        <div class="w3-bar-block">
            <a href="#" class="w3-bar-item w3-button w3-padding-16 w3-hide-large w3-dark-grey w3-hover-black" onclick="w3_close()" title="close menu"><i class="fa fa-remove fa-fw"></i>  Close Menu</a>
            <a href="info" class="w3-bar-item w3-button w3-padding w3-blue"><i class="fa fa-users fa-fw"></i>настройки аккаунта </a>
            <a href="ManageLogins" class="w3-bar-item w3-button w3-padding w3-blue"><i class="fa fa-users fa-fw"></i> Изменить логин</a>
            <a href="Rent.html" class="w3-bar-item w3-button w3-padding w3-blue"><i class="fa fa-users fa-fw"></i> Арендовать место</a>
        </div>
    </nav>



    <div class="w3-overlay w3-hide-large w3-animate-opacity" onclick="w3_close()" style="cursor:pointer" title="close side menu" id="myOverlay"></div>

    <div class="w3-main" style="margin-left:300px;margin-top:43px;">

        <header class="w3-container" style="padding-top:22px">
            <h5><b><i class="fa fa-dashboard"></i> Основная панель</b></h5>
        </header>
        <div class="w3-row-padding w3-margin-bottom">
            <div class="w3-quarter">
                <div class="w3-container w3-red w3-padding-16">

                    <div class="w3-right">
                        <h3>Доступно </h3>
                    </div>
                    <div class="w3-clear"></div>
                    <h4>Свободных мест</h4>
                </div>
            </div>
            <div class="w3-quarter">
                <div class="w3-container w3-blue w3-padding-16">
                    <div class="w3-right">
                        <h3>@*@Model.placeId*@</h3>
                    </div>

                    <div class="w3-clear"></div>
                    <h4>Ваше место</h4>
                </div>
            </div>
            <div class="w3-quarter">
                <div class="w3-container w3-teal w3-padding-16">

                    <div class="w3-right">
                        <h3>@*@Model.price*@</h3>
                    </div>
                    <div class="w3-clear"></div>
                    <h4>Итог к оплате</h4>
                </div>
            </div>
            <div class="w3-quarter">
                <div class="w3-container w3-black w3-padding-16">

                    <div class="w3-right">
                        <h3>@*@Model.mark*@</h3>
                    </div>
                    <div class="w3-clear"></div>
                    <h4>ваше авто</h4>
                </div>
            </div>

        </div>

        <div class="w3-panel">
            <div class="w3-row-padding" style="margin:0 -16px">

                <div class="w3-twothird">
                    <h5>Последние ваши действия</h5>
                    <table class="w3-table w3-striped w3-white">



                        <tr>
                            <td>Аренда места</td>
                            <td><i>25 минут</i></td>
                        </tr>
                        <tr>
                            <td>Успешная авторизация.</td>
                            <td><i>28 минут</i></td>

                    </table>
                </div>
            </div>
        </div>

    </div>

</body>
</html>











